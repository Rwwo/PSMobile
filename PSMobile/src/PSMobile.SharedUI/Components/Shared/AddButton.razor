@using MudBlazor;

<MudLink Href="@Href" Color="Color.Info" Class="mb-4">
    <MudButton Variant="Variant.Filled"
               Color="Color.Info"
               StartIcon="@Icons.Material.Filled.Add"
               Size="Size.Large"
               Class="mb-4">
        @Text
    </MudButton>
</MudLink>


@code {
    [Parameter] public string? Href { get; set; }
    [Parameter] public string? Text { get; set; }
}
